{
  "presets": ["react", "es2015"],
  "env": {
    "development": {
      "presets": ["react-hmre"],
      "plugins": [["react-transform", {
        "transforms": [{
          "transform": "react-transform-hmr",
          "imports": ["react"],
          "locals": ["module"]
        }]
      }]]
    
    }
  },
  "plugins": [
    "transform-flow-strip-types"
  ]
}
